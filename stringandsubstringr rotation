#include <stdio.h>
#include <string.h>

int main() {
    char s[100], x[100];
    int i, j, len1, len2, match;

    printf("Enter the string:");
    scanf("%s", s);

    printf("Enter the substring:");
    scanf("%s", x);

    len1 = strlen(s);
    len2 = strlen(x);

    if(len1 != len2) {
        printf("false");
        return 0;
    }

    for(i = 0; i < len1; i++) {
        match = 1;

        for(j = 0; j < len1; j++) {
            if(s[(i + j) % len1] != x[j]) {
                match = 0;
                break;
            }
        }

        if(match == 1) {
            printf("true");
            return 0;
        }
    }

    printf("false");
    return 0;
}
